{
  "name": "wetube",
  "version": "1.0.0",
  "engines": {
    "npm": "6.x",
    "node": "10.x"
  },
  "description": "Cloning Youtube by vanilla and nodeJS",
  "author": "khai",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "aws-sdk": "^2.543.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "eslint-config-prettier": "^6.3.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.0",
    "shx": "^0.3.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "dev:server": "nodemon --delay 2 --exec babel-node src/init.js --ignore `scss`",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "shx cp -r src/static build/static && shx cp -r src/views build/views",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "cross-env": "^5.2.1",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^1.19.2"
  }
}
